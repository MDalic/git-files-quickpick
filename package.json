{
  "name": "git-files-quickpick",
  "displayName": "Git Files QuickPick",
  "description": "QuickPick modal to view and open staged and changed Git files with icons.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.70.0"
  },
  "activationEvents": [
    "onCommand:extension.showChangedFiles"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.showChangedFiles",
        "title": "Show Changed and Staged Git Files"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "^16.x",
    "@types/vscode": "^1.70.0",
    "typescript": "^4.8.0",
    "@vscode/test-electron": "^2.3.1"
  }
}